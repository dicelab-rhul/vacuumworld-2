{

@possibleactions : {
	communicationaction	: uk.ac.rhul.cs.dice.starworlds.actions.environmental.CommunicationAction,
	sensingaction		: uk.ac.rhul.cs.dice.starworlds.actions.environmental.SensingAction,
	moveaction		: uk.ac.rhul.cs.dice.vacuumworld.actions.MoveAction,
	turnaction		: uk.ac.rhul.cs.dice.vacuumworld.actions.TurnAction,
},

@environments : {
	universe : uk.ac.rhul.cs.dice.vacuumworld.VacuumWorldEnvironment,
},

@appearances : {
	environmentalappearance : uk.ac.rhul.cs.dice.starworlds.appearances.EnvironmentalAppearance,
},

@physics : {
	physics :uk.ac.rhul.cs.dice.vacuumworld.VacuumWorldPhysics,
},

@states : {
	state : uk.ac.rhul.cs.dice.vacuumworld.VacuumWorldState,
},

@agentminds : {
	mind : uk.ac.rhul.cs.dice.vacuumworld.agent.VacuumWorldAgentMind,
},

@agentbodies : {
	agent :  uk.ac.rhul.cs.dice.vacuumworld.agent.VacuumWorldAgent,
},

@agentsensors : {
	seeingsensor : uk.ac.rhul.cs.dice.vacuumworld.agent.VacuumWorldSeeingSensor,
	listeningsensor : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.ListeningSensor,
},

@agentactuators : {
	physicalactuator : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.PhysicalActuator,
	speechactuator   : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.SpeechActuator,
},

~agents : {
	VacuumWorldAgent: {
		@agentbodies    : agent,
		@agentminds     : mind,
		@agentsensors   : [listeningsensor, seeingsensor],
		@agentactuators : [physicalactuator, speechactuator],
	},
},

~environments : {
	Universe : {
		@environments 	 : universe,
		@possibleactions : [communicationaction, sensingaction, turnaction, moveaction],
		@physics         : physics,
		@states          : state,
	},
},

~structure : [{
		~environments : Universe,
		~agents : [VacuumWorldAgent],
	},
]
}